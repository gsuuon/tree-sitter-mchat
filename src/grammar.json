{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mchat",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "chat_handler"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "params_block"
                },
                {
                  "type": "SYMBOL",
                  "name": "_newline"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "system_instruction"
                },
                {
                  "type": "SYMBOL",
                  "name": "_newline"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "message_sequence"
        }
      ]
    },
    "chat_handler": {
      "type": "SYMBOL",
      "name": "_line"
    },
    "params_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "---"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "SYMBOL",
          "name": "_lines"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "STRING",
          "value": "---"
        }
      ]
    },
    "system_instruction": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": 2,
            "content": {
              "type": "STRING",
              "value": "> "
            }
          }
        },
        {
          "type": "SYMBOL",
          "name": "_line"
        }
      ]
    },
    "message_sequence": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "user_message"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "turn_delimiter"
              },
              {
                "type": "SYMBOL",
                "name": "assistant_message"
              },
              {
                "type": "SYMBOL",
                "name": "turn_delimiter"
              },
              {
                "type": "SYMBOL",
                "name": "user_message"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "turn_delimiter"
                },
                {
                  "type": "SYMBOL",
                  "name": "assistant_message"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "turn_delimiter"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "user_message": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SYMBOL",
        "name": "_lines"
      }
    },
    "content": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": ".+"
      }
    },
    "assistant_message": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "data_section"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "content"
        }
      ]
    },
    "data_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "<<<<<<(?: .+)?\\n"
          },
          "named": true,
          "value": "data_section_open"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": ".+"
            }
          },
          "named": true,
          "value": "data_section_body"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "TOKEN",
            "content": {
              "type": "PREC",
              "value": 2,
              "content": {
                "type": "STRING",
                "value": ">>>>>>"
              }
            }
          },
          "named": true,
          "value": "data_section_close"
        }
      ]
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "_line": {
      "type": "PATTERN",
      "value": ".+"
    },
    "_lines": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_line"
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          }
        ]
      }
    },
    "turn_delimiter": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": 2,
            "content": {
              "type": "STRING",
              "value": "======"
            }
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
